/* ==========================================================================
   Base Layout (small desktop, laptop, 9" landscape) | For 1024 screen size
   ========================================================================== */

/*
 * 1. Sets root font-size for sizing with rem units.
 * 2. For sticky footer.
 */

html {
  font-size: 20px;
  height: 100%;
}

/*
 * 1. For sticky footer.
 * 2. Forces scrollbar to avoid jumpiness where content is light.
 * 3. Fallback for IE.
 * 4. Fallback for IE.
 * 5. Sets default font-size. Cascades through document.
 * 6. Sets default line-height. Cascades through document. Establishes baseline grid.
 * 7. Kills unwanted margins from UA style sheet.
 */

body {
  height: 100%; /* 1 */
  overflow-y: scroll; /* 2 */

  /*IE*/font-size: 20px; /* 3 */
  /*IE*/line-height: 30px; /* 4 */

  font-size: 1rem; /* 5 */
  line-height: 1.5rem; /* 6 */
  margin: 0; /* 7 */
}

dl,
dd,
dt,
ol,
p,
ul,
pre {
  /*IE*/margin-bottom: 30px;

  margin-top: 0;
  margin-bottom: 1.5rem;
}

/*
 * Prevents a double margin-bottom.
 */

dl ol,
dl ul {
  margin-bottom: 0;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: 'OswaldBook', "Helvetica Neue", Helvetica, Arial, sans-serif;
  /*IE*/margin: 0 0 30px 0;

  font-weight: normal;
  margin: 0 0 1.5rem 0;
}
h1 {
  /*IE*/font-size: 100px;
  /*IE*/line-height: 90px;
  /*IE*/letter-spacing: -2px;

  font-size: 5rem; /* 3 lines of text, aligns optically from baseline to cap height */
  line-height: 4.5rem; /* 3x line-height set on <body> */
  letter-spacing: -.1rem;
}
h2 { 
  /*IE*/font-size: 60px;
  /*IE*/line-height: 60px;

  font-size: 3rem; /* 2 lines of text, aligns optically from baseline to cap height */
  line-height: 3rem; /* 2x of line-height set on <body> */
}
h3 {
  /*IE*/font-size: 20px;
  font-size: 1rem; /* Same as <body> */
}
h4 {
  /*IE*/font-size: 18px;
  font-size: .9rem;
}
h5 {
  /*IE*/font-size: 16px;
  font-size: .8rem;
}
h6 {
  /*IE*/font-size: 14px;
  font-size: .7rem;
}



/* Sticky Footer
   ========================================================================== */

.l-wrap {
  min-height: 100%;
}

.l-content {
  /*IE*/padding-bottom: 120px;

  padding-bottom: 6rem;
}

.l-footer {
  /*IE*/margin-top: -120px;
  /*IE*/padding-top: 30px;
  /*IE*/height: 120px;

  margin-top: -6rem;
  padding-top: 1.5rem;
  height: 6rem;
}



/* Page Sections
   ========================================================================== */

.l-header {
  /*IE*/height: 75px;
  height: 3.75rem;
  padding-top: 0;
}
  .l-header * {
    margin-bottom: 0;
  }

.l-header-int {
  /*IE*/padding-top: 60px;
  /*IE*/padding-bottom: 150px;

  padding-top: 3rem;
  padding-bottom: 7.5rem;
}



/* ==========================================================================
   Grid
   ========================================================================== */

.l-bound {
  /*IE*/width: 930px;
  /*IE*/padding-right: 40px;
  /*IE*/padding-left: 40px;

  width: 46.5rem; 
  padding-right: 2rem;
  padding-left: 2rem;
  margin: 0 auto;
}

/*
 * For containers containing .l-col's
 * Uses Micro Clearfix to clear floats
 */

.l-row {}

/* 
 * Uses Micro Clearfix by Nicolas Gallagher 
 * http://nicolasgallagher.com/micro-clearfix-hack/
 */

  .l-bound:before,
  .l-bound:after,
  .l-row:before,
  .l-row:after {
    content: " ";
    display: table;
  }
  .l-bound:after,
  .l-row:after {
    clear: both;
  }
  .l-bound,
  .l-row {
    *zoom: 1;
  }

.l-col {
  /*IE*/margin-left: 30px;

  float: left;
  margin-left: 1.5rem;

  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}

/* 
 * A container will be needed for a new row
 */

.l-col:first-child {
  margin-left: 0;
}

/* 
 * Extender class to reverse the float
 */

.l-col\+r {
  float: right;
}

/*
 * col + margin-left, etc. = 4rem
 */

.l-col\+1  { /*IE*/width: 50px; width: 2.5rem; }
.l-col\+2  { /*IE*/width: 130px; width: 6.5rem; }
