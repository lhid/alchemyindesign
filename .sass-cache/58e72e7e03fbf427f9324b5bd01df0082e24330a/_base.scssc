 */

textarea {
    overflow: auto; /* 1 */
    vertical-align: top; /* 2 */
    min-height: 7.5rem;
}

/*
 * Creates padding while maintaining a total width of 100%.
 */

textarea,
[type='text'],
[type='email'] {
    /*IE*/padding: 10px;
    padding: .5rem;
    width: 100%;
}

/*
 * Height = 2 line-heights to maintain baseline grid.
 * Defaults width to 100% of container.
 */

button,
[type='submit'] {
    /*IE*/height: 60px;
    height: 3rem;
    width: 100%;
}

/*
 * Maintains baseline grid.
 */

textarea,
[type='text'],
[type='email'],
button,
[type='submit'] {
  /*IE*/margin-bottom: 30px;
  margin-bottom: 1.5rem;
}


/* ==========================================================================
   Tables
   ========================================================================== */

/*
 * Remove most spacing between table cells.
 */

table {
    border-collapse: collapse;
    border-spacing: 0;
}



